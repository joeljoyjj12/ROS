<launch>

<rosparam file="$(find jr_final_robot_description)/launch/controller.yaml" command="load"/>
<node args="Rev17_position_controller Rev18_position_controller joint_state_controller " name="controller_spawner" ns="jr_final_robot" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/jr_final_robot/joint_states"/>
</node>

</launch>